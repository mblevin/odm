$('div#save-message').show().text("<%= @message %>");
if (
  "<%= @message %>" === "Event not saved. Try again." ||
  "<%= @message %>" === "You don't have permission to do that.") {}
  else{
  $('div#save-message').slideDown().text("<%= @message %>");
  var events = [];

  <% @events.each do |event| %>
    events.push({'title': '<%=j event.title %>', 'placeName': '<%=j event.place_name %>'})
  <% end %>

  renderExistingEvents(events);
  resetNewEventPage();
};